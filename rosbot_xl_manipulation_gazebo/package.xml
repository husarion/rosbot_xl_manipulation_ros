<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>rosbot_xl_manipulation_gazebo</name>
  <version>1.1.0</version>

  <description>The rosbot_xl_manipulation_gazebo package</description>
  <license>Apache License 2.0</license>

  <author email="maciej.stepien@husarion.com">Maciej Stępień</author>
  <maintainer email="contact@husarion.com">Husarion</maintainer>

  <url type="website">https://husarion.com/</url>
  <url type="repository">https://github.com/husarion/rosbot_xl_manipulation_ros</url>
  <url type="bugtracker">https://github.com/husarion/rosbot_xl_manipulation_ros/issues</url>
  
  <buildtool_depend>ament_cmake</buildtool_depend>
  
  <exec_depend>rosbot_xl_manipulation_bringup</exec_depend>

  <exec_depend>launch</exec_depend>
  <exec_depend>launch_ros</exec_depend>

  <exec_depend>husarion_gz_worlds</exec_depend>
  <exec_depend>ros_gz_sim</exec_depend>
  <!-- Ignition dependency is specified in the ros_gz_sim package, 
    version can chosen using GZ_VERSION (or IGNITION_VERSION) env variable, 
    for details refer to the ros_gz_sim package -->
  <exec_depend>ros_gz_bridge</exec_depend>
  <exec_depend>ign_ros2_control</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>